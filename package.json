  },
  "scripts": {
    "clean": "find . -type d -empty -delete && git clean -Xfd -e .env.local && pnpm i",
    "knip": "knip -c knip.config.ts",
    "nx": "nx",
    "build": "nx run-many -t build",
    "format": "prettier . -w --cache",
    "lint": "nx run-many -t lint",
    "test": "nx run-many -t test",
    "typecheck": "nx run-many -t typecheck",
    "e2e": "nx run-many -t e2e",
    "remove-built": "fd -e \".d.ts\" -e \".js\" --exclude \"jest.preset.js\" --exclude \"env.d.ts\" --exclude \"repokit/\" -x rm {}",
    "all": "nx run-many -t=build,lint,test,typecheck"
  },
  "version": "1.0.0",
  "workspaces": ["packages/*"]
