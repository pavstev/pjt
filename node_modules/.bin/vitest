#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/stevan/work/package-json-tools/node_modules/.pnpm/vitest@4.0.14_@types+node@24.10.1_jiti@2.6.1_yaml@2.8.1/node_modules/vitest/node_modules:/home/stevan/work/package-json-tools/node_modules/.pnpm/vitest@4.0.14_@types+node@24.10.1_jiti@2.6.1_yaml@2.8.1/node_modules:/home/stevan/work/package-json-tools/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/stevan/work/package-json-tools/node_modules/.pnpm/vitest@4.0.14_@types+node@24.10.1_jiti@2.6.1_yaml@2.8.1/node_modules/vitest/node_modules:/home/stevan/work/package-json-tools/node_modules/.pnpm/vitest@4.0.14_@types+node@24.10.1_jiti@2.6.1_yaml@2.8.1/node_modules:/home/stevan/work/package-json-tools/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/vitest@4.0.14_@types+node@24.10.1_jiti@2.6.1_yaml@2.8.1/node_modules/vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../.pnpm/vitest@4.0.14_@types+node@24.10.1_jiti@2.6.1_yaml@2.8.1/node_modules/vitest/vitest.mjs" "$@"
fi
